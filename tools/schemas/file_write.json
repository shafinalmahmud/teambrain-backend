{
  "name": "file_write",
  "description": "Write content to a file",
  "input_schema": {
    "type": "object",
    "required": ["path", "content"],
    "properties": {
      "path": { "type": "string" },
      "content": { "type": "string" }
    }
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "success": { "type": "boolean" }
    }
  },
  "error_schema": {
    "type": "object",
    "properties": {
      "error": { "type": "string" },
      "reason": { "type": "string" }
    }
  },
  "metadata": {
    "version": "1.0.0",
    "permissions": ["Executor"],
    "rate_limit": { "max_calls": 20, "per_seconds": 60 },
    "safety": { "disallowed_topics": [], "sensitive_fields": [] }
  }
}